pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                sh 'node -v'
                sh 'npm -v'
            }
        }
        stage('Test'){
            steps{
                sh 'java --version'
            }
        }
        stage('Deploy'){
            steps{
                sh 'java --version'
            }
        }
    }
}